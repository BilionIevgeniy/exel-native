{
	"parser": "babel-eslint",
	"env": {
		"es6": true,
		"browser": true,
		"node": true
	},
	"rules": {
		"no-tabs": "off",
		"indent": ["error", "tab", {"SwitchCase": 1}],
		"object-curly-spacing": "off",
		"require-jsdoc": "off",
		"eol-last": "off",
		"max-len": "off",
		"quotes": ["error", "single"]
	},
	"overrides": [
		{
			"files": ["*.js"]
		}
	],
	"extends": ["eslint:recommended", "google"]
}
